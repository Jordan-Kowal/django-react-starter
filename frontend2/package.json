{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "homepage": "/",
  "type": "module",
  "scripts": {
    "biome:check": "biome check ./src",
    "i18n": "npx i18next 'src/**/*.{ts,tsx}' --silent",
    "i18n:check": "npx i18next 'src/**/*.{ts,tsx}' --silent --fail-on-update && node ./i18n/check-empty-translations.cjs",
    "quality": "yarn biome:check && yarn i18n:check && yarn tsc",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "(export NODE_NO_WARNINGS=1 && vitest --silent)",
    "test:watch": "(export NODE_NO_WARNINGS=1 && vitest --watch --silent)",
    "test:ui": "(export NODE_NO_WARNINGS=1 && vitest --ui --silent)",
    "test:coverage": "(export NODE_NO_WARNINGS=1 && vitest --coverage --silent)",
    "tsc": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-query": "5.68.0",
    "@tanstack/react-router": "^1.114.17",
    "dayjs": "1.11.13",
    "i18next": "^24.2.3",
    "jkscript": "3.3.0",
    "million": "3.1.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-toastify": "^11.0.5",
    "zod": "^3.24.2",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/router-plugin": "^1.114.17",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "daisyui": "^5.0.4",
    "i18next-parser": "^9.3.0",
    "jsdom": "26.0.0",
    "msw": "2.7.3",
    "tailwindcss": "^4.0.14",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "vite": "6.2.2",
    "vitest": "3.0.8"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
